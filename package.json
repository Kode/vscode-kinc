{
	"name": "kinc",
	"displayName": "Kinc",
	"description": "Kinc support",
	"version": "20.10.1",
	"publisher": "kodetech",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Kinc",
		"Kore"
	],
	"activationEvents": [
		"workspaceContains:kincfile.js",
		"onCommand:kinc.init",
		"onCommand:kinc.compile"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "kinc.init",
				"title": "Init Project",
				"category": "Kinc"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Kinc",
			"properties": {
				"kinc.kincPath": {
					"type": "string",
					"default": "",
					"description": "Points to a Kinc directory."
				},
				"kinc.ffmpeg": {
					"type": "string",
					"default": "",
					"description": "Path to ffmpeg for extended audio and video format support."
				},
				"kinc.buildDir": {
					"type": "string",
					"default": "build",
					"description": "Relative path to the Kinc build dir for the project."
				}
			}
		},
		"taskDefinitions": [
			{
				"type": "Kinc",
				"required": [
					"target"
				],
				"properties": {
					"target": {
						"type": "string",
						"description": "The target to build for."
					}
				}
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kode/vscode-kinc"
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"eslint": "^4.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.36"
	},
	"extensionDependencies": [
		"ms-vscode.cpptools"
	]
}
